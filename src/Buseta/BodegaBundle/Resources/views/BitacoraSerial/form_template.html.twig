<form id="{{ form.vars.id }}" name="{{ form.vars.full_name }}" action="{{ form.vars.action }}" method="POST" enctype="multipart/form-data">
    {% if form.vars.method == 'PUT' %}
        <input type="hidden" name="_method" value="PUT"/>
    {% endif %}
    {{ form_errors(form) }}
    {{ form_row(form.id) }}
    {{ form_row(form._token) }}

    <div class="row">
        <div class="col-md-4">
            {{ form_row(form.cantMovida) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.fechaMovimiento) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.tipoMovimiento) }}
        </div>
        <div class="col-md-4">
            {{ form_row(form.producto) }}
        </div>
    </div>

    {# buttons group #}
    <div class="row">
        <div class="col-md-6">
            <a class="btn btn-default" id="btn_producto_cancel" href="{{ path('bitacoraalmacen') }}"><span class="glyphicon glyphicon-chevron-left"></span>
                {{ 'actions.cancel' | trans({}, 'BusetaBodegaBundle') }}</a>
        </div>
    </div>
</form>
